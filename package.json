{
  "name": "@warp-works/warpjs-action-plugin",
  "version": "1.2.0",
  "description": "Base action plugin class",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run -s lint && npm run -s test",
    "coverage": "npm run -s coverage:unit",
    "coverage:unit": "nyc --report-dir=reports/coverage/unit npm run -s test:unit",
    "lint": "npm run -s lint:node && npm run -s lint:test",
    "lint:node": "eslint --fix --config=.eslintrc.node.json --ignore-pattern \"*.test.js\" *.js lib",
    "lint:test": "eslint --fix --config=.eslintrc.test.json \"lib/**/*.test.js\" \"*.test.js\"",
    "test": "npm run -s test:unit",
    "test:unit": "mocha \"*.unit.test.js\" \"lib/**/*.unit.test.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WarpWorks/warpjs-action-plugin.git"
  },
  "keywords": [
    "warp-works",
    "warpjs",
    "action",
    "plugins"
  ],
  "author": {
    "name": "Hữu Ðà Trần",
    "email": "huuda.tran@quoininc.com",
    "url": "https://www.quoininc.com/staff/huu-da-tran/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WarpWorks/warpjs-action-plugin/issues"
  },
  "homepage": "https://github.com/WarpWorks/warpjs-action-plugin#readme",
  "dependencies": {
    "@warp-works/warpjs-plugin": "1.2",
    "lodash": "4.17.x"
  },
  "devDependencies": {
    "@quoin/eslint-config-quoin": "latest",
    "@quoin/node-test-helpers": "1",
    "eslint": "4",
    "eslint-plugin-html": "4.0",
    "eslint-plugin-import": "2.13",
    "eslint-plugin-json": "1.2",
    "eslint-plugin-node": "6.0",
    "eslint-plugin-promise": "3.8",
    "eslint-plugin-standard": "3.1",
    "mocha": "5",
    "nyc": "12.0"
  }
}
